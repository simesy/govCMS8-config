{
    "name": "govcms/govcms8-config",
    "description": "Configuration for the govCMS Drupal 8 distribution",
    "keywords": ["govCMS", "govCMS8"],
    "type": "composer-plugin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Joseph Zhao",
            "email": "joseph.zhao@xing.net.au"
        },
        {
            "name": "Jason Guo",
            "email": "jason.guo@xing.net.au"
        },
        {
            "name": "Tim Hurley",
            "email": "tim@timhurley.net"
        },
        {
            "name": "ruwanl",
            "email": "ruwan.lk@gmail.com"
        }
    ],
    "homepage": "https://www.govcms.gov.au",
    "support": {
        "issues:": "https://github.com/govCMS/govCMS8-config/issues",
        "wik": "https://github.com/govCMS/govCMS8-config/wiki",
        "source": "https://github.com/govCMS/govCMS8-config/releases"
    },
    "require": {
        "composer-plugin-api": "^1.1",
        "consolidation/robo": "^1.1",
        "consolidation/config": "^1.0",
        "php": "^5.6.0 || ^7.0",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "composer/composer": "^1.5"
